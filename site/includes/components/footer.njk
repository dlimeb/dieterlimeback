<footer id="footer" class="w-full max-w-6xl mx-auto mb-32" role="contentinfo">
  <div class="mx-4 md:mx-8 border-t border-solid divider-gray-200 px-2 pt-1">
    <div class="text-center">
      <ul class="mb-1">
        <li class="">
          <a href="/contact/" class="text-brand-darkblue font-bold">Book a call</a> <span class="hidden md:inline">•</span>
        </li>
        {% for site, link in site.social_links %}
          <li class="">
            <a href="{{ link | safe }}">
              {{ site }}
            </a>
          </li>
        {% endfor %}
      </ul>
    </div>
    <div class="mt-4 text-center font-sans text-base text-coolGray-400">
      © 2020
    </div>
  </div>
</footer>